      this.setState({\n        isFinish: true,\n      });\n    }\n\n    // document.getElementById('details').innerHTML = '';\n  };\n\n  handleRandomIndexArray = () => {\n    const indexArray = [];\n    // 1. 정답인 testIndex는 반드시 포함이 되어야 하므로 array에 push해준다.\n    indexArray.push(this.state.testIndex);\n\n    // 2. 사지선다이므로 array 길이가 4가 될 때 까지 반복\n    while (indexArray.length < 4) {\n      const randomIndex = Math.floor(Math.random() * this.state.testWords.length);\n      // 2-1. 중복된 숫자는 넣으면 안되므로 indexOf가 -1일때만 push\n      if (indexArray.indexOf(randomIndex) == -1) {\n        indexArray.push(randomIndex);\n      }\n    }\n\n    // 3. 배열을 섞지 않으면 정답이 무조건 1번에 오기 때문에 한 번 섞어준다\n    for (let i = indexArray.length - 1; i >= 0; --i) {\n      let j = Math.floor(Math.random() * (i + 1));\n      [indexArray[i], indexArray[j]] = [indexArray[j], indexArray[i]];\n    }\n\n    // 4. randomIndexArray에 넣어준다\n    this.setState({ randomIndexArray: indexArray });\n    this.setState({ isLoading: false });\n  };\n\n  render() {\n    const { runningTime, score, testIndex, isFinish, wrongWords, testWords, randomIndexArray, isLoading } = this.state;\n    const minute = this.timeFormatter(runningTime.minute);\n    const second = this.timeFormatter(runningTime.second);\n    return (\n      <div>\n        {isLoading ? (\n          <div>로딩중입니다...</div>\n        ) : (\n          <div>\n            <h1>단어 테스트</h1>\n            <div className=\"timer\">{isFinish ? '' : minute + ':' + second}</div>\n\n            {isFinish ? (\n              <TestResult score={score} minute={minute} second={second} wrongWords={wrongWords} />\n            ) : (\n              <div>\n                <div id=\"details\"></div>\n                <div>\n                  <div>{testWords[testIndex].voca}</div>\n                </div>\n                <div onClick={this.handleClickAns}>{testWords[randomIndexArray[0]].meaning}</div>\n                <div onClick={this.handleClickAns}>{testWords[randomIndexArray[1]].meaning}</div>\n                <div onClick={this.handleClickAns}>{testWords[randomIndexArray[2]].meaning}</div>\n                <div onClick={this.handleClickAns}>{testWords[randomIndexArray[3]].meaning}</div>\n                <button onClick={this.handleNextWord}>버튼</button>\n                <div>{testIndex / testWords.length}</div>\n              </div>\n            )}\n          </div>\n        )}\n      </div>\n    );\n  }\n}\n\nexport default ChoiceTest;\n","/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/flashTest/FlashTest.js",[],"/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/flashTest/TestResult.js",[],"/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/flashTest/OnTest.js",[],"/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/blinkerStudy/BlinkerStudy.js",[],"/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/choiceTest/OnTest.js",[],"/Users/seunghye/Desktop/my-app 2/src/Components/WordTest/choiceTest/TestResult.js",[],{"ruleId":"182","replacedBy":"183"},{"ruleId":"184","replacedBy":"185"},{"ruleId":"182","replacedBy":"186"},{"ruleId":"184","replacedBy":"187"},{"ruleId":"188","severity":1,"message":"189","line":1,"column":17,"nodeType":"190","messageId":"191","endLine":1,"endColumn":26},{"ruleId":"188","severity":1,"message":"189","line":1,"column":17,"nodeType":"190","messageId":"191","endLine":1,"endColumn":26},{"ruleId":"188","severity":1,"message":"192","line":4,"column":8,"nodeType":"190","messageId":"191","endLine":4,"endColumn":13},{"ruleId":"188","severity":1,"message":"193","line":2,"column":8,"nodeType":"190","messageId":"191","endLine":2,"endColumn":14},{"ruleId":"194","severity":1,"message":"195","line":173,"column":43,"nodeType":"196","messageId":"197","endLine":173,"endColumn":45},"no-native-reassign",["198"],"no-negated-in-lhs",["199"],["198"],["199"],"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","'axios' is defined but never used.","'OnTest' is defined but never used.","eqeqeq